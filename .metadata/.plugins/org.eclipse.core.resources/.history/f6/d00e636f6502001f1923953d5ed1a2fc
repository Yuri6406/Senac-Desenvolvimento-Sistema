package exercicios;

import java.util.Scanner;
import java.math.*;

public class Ex07 {

	public static void main(String[] args) {

		Scanner leia = new Scanner(System.in);
		
		int num = leia.nextInt();
		
		for (int i = 0; i < num; i++) {
			int fat = num*(num-i);
			int res = fat+ fat;
			System.out.println(res);
		}
	}
}
