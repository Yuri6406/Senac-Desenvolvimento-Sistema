package exercicios;

import java.util.Scanner;

public class Ex04 {

	public static void main(String[] args) {
		Scanner leia = new Scanner(System.in);

		System.out.println("Digite o nÃºmero que deseja verificar: ");

		int num = leia.nextInt();

		while (num > 0) {
			if (num % 2 != 0) {
				System.out.println(num);
			}else{
				num--;
			}
		}
	}
}
