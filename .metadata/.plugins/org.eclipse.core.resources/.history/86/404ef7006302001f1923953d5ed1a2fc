package exercicios;

import java.util.Scanner;
import java.math.*;

public class Ex06 {

	public static void main(String[] args) {

		Scanner leia = new Scanner(System.in);
		
		int num = leia.nextInt();
		
		for (int i = 1; i <= num; i++) {
			
			int raiz2 = (int) Math.pow(i,2);
			
			
			System.out.println(i + " " + raiz2 + " " + (int) Math.pow(i, 3));
		}
		
	}

}
