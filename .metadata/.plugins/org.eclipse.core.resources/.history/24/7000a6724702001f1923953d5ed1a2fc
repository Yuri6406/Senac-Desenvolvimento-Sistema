package exercicios;

import java.util.Scanner;

public class Ex04 {

	public static void main(String[] args) {
		Scanner leia = new Scanner(System.in);

		System.out.println("Digite o n√∫mero que deseja verificar: ");

		int num = leia.nextInt();

		for (int i = 1; i <= num; i++) {

			if (num % 2 != 0) {
				System.out.println(num);
				num--;
			}
		}
	}
}
