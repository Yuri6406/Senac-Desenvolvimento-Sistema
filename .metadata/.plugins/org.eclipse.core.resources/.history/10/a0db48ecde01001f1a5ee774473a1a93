package exercicios;

import java.util.Scanner;

public class Ex04 {

	public static void main(String[] args) {
		Scanner leia = new Scanner(System.in);
		
		System.out.println("Digite o n√∫mero que deseja verificar: ");
		int num = leia.nextInt();

		for (int i = 0; i <= num ; i++) {
			if (num % i != 0) {
				int res = num / i;
				System.out.println(res);
			}
		}
		
		
	}

}
